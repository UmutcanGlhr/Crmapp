@model IEnumerable<Meet>


<div class="profile">
    <aside>
        <partial name="_profileSideBar" />
    </aside>

    <div class="profile_left">
        <h1>Randevular</h1>
        @if (Model.Count() > 0)
        {
            @foreach (var mt in Model)
            {
                <partial name="_myMeetsDetail" model="@mt" />
            }
        }
    </div>
</div>