@model IEnumerable<Meet>
@{
    Layout = null;
}
<link href="~/css/bootstrap.min.css" rel="stylesheet">
<div class="row" style="margin-top: 2.3rem;">
    <div class="col-md-2" style="">
        <partial name="_ProfilSideBar" />
    </div>
    <div class="col-md-7" style="margin-left:100px;">
        <h3>Randevular</h2>
            @if (Model.Count() > 0)
            {
                <div class="my-3">
                    @foreach (var meet in Model)
                    {

                        <partial name="_myMeetDetail" model="@meet" />

                    }
                </div>
            }
    </div>
</div>