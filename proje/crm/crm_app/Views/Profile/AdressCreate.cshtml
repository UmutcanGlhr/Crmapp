@model AdressDtoForInsertion


<div class="profile">
    <aside>
        <partial name="_profileSideBar" />
    </aside>

    <div class="profile_left">
        <form asp-action="AdressCreate" method="post">
            <div class="form-group my-3 ">
                <label asp-for="CityName"></label>
                <input asp-for="CityName" class="form-control" />
            </div>
            <div class="form-group my-3 ">
                <label asp-for="DistrictName"></label>
                <input asp-for="DistrictName" class="form-control" />
            </div>
            <div class="form-group my-3 ">
                <label asp-for="AddressName"></label>
                <input asp-for="AddressName" class="form-control" />
            </div>
            <div class="form-group my-3 ">
                <label asp-for="ZipCode"></label>
                <input asp-for="ZipCode" class="form-control" />
            </div>
            <button class="btn btn-success" type="submit">
                <i class="fa fa-save"></i>
                Save
            </button>
        </form>
    </div>
</div>
