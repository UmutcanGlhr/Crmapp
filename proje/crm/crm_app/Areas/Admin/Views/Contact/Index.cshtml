@model IEnumerable<Contact>

<div class="text-center my-3" style="margin-top: 1rem;">
    <h1>Mesajlar</h1>
</div>

<div class="my-3">
    <table class="table  table-hover">
        <thead>
            <tr>
                <th scope="col">isim </th>
                <th scope="col">Email</th>
                <th scope="col">Konu</th>
                <th scope="col">Mesaj</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var ct in Model)
            {
                <tr>
                    <th scope="row">@ct.isim</th>
                    <th scope="row">@ct.Email</th>
                    <th scope="row">@ct.Konu</th>
                    <th scope="row">@ct.Mesaj</th>
                </tr>

            }



        </tbody>
    </table>
</div>