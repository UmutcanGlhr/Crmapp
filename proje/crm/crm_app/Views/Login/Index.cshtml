@model LoginModel


<h1>Login</h1>
<div asp-validation-summary="All"></div>
<form method="post" asp-action="Index">

    <label asp-for="Email"></label>
    <input asp-for="Email">

    <p>&nbsp;</p>
    <label asp-for="Password"></label>
    <input asp-for="Password" type="password">
    <p>&nbsp;</p>

    <button type="submit">Login</button>
</form>